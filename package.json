{
  "name": "zigbee-faker",
  "version": "0.0.1",
  "description": "Generate deterministic fake data for Zigbee",
  "license": "GPL-3.0-or-later",
  "author": "Nerivec",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./generics": "./dist/generics.js",
    "./rng": "./dist/rng.js",
    "./zigbee": "./dist/zigbee.js",
    "./zigbee2mqtt": "./dist/zigbee2mqtt.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerivec/zigbee-faker.git"
  },
  "bugs": {
    "url": "https://github.com/Nerivec/zigbee-faker/issues"
  },
  "homepage": "https://github.com/Nerivec/zigbee-faker/#readme",
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "prepack": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/json-stable-stringify-without-jsonify": "^1.0.2",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14",
    "zigbee-herdsman": "^7.0.4",
    "zigbee-herdsman-converters": "^25.84.0",
    "zigbee2mqtt": "github:Koenkk/zigbee2mqtt#dev-types"
  },
  "keywords": [
    "zigbee",
    "faker",
    "fake-data",
    "deterministic-fake",
    "deterministic-data",
    "random-data",
    "zigbee2mqtt",
    "zigbee-herdsman"
  ]
}